    <script>
        document.addEventListener('DOMContentLoaded', () => {

            const navLinks = document.querySelectorAll('.nav-link');
            const mobileNav = document.getElementById('mobile-nav');
            const contentSections = document.querySelectorAll('.content-section');
            const objectiveLinks = document.querySelectorAll('.objective-card');
            
            function switchTab(sectionId) {
                contentSections.forEach(section => {
                    section.classList.toggle('active', section.id === sectionId);
                });

                navLinks.forEach(link => {
                    link.classList.toggle('active', link.dataset.section === sectionId);
                });
                
                if (mobileNav.value !== sectionId) {
                    mobileNav.value = sectionId;
                }
            }

            navLinks.forEach(link => {
                link.addEventListener('click', (e) => {
                    e.preventDefault();
                    const sectionId = e.target.dataset.section;
                    switchTab(sectionId);
                    history.pushState(null, '', `#${sectionId}`);
                });
            });

            mobileNav.addEventListener('change', (e) => {
                 const sectionId = e.target.value;
                 switchTab(sectionId);
                 document.getElementById(sectionId).scrollIntoView({ behavior: 'smooth' });
            });
            
            objectiveLinks.forEach(link => {
                link.addEventListener('click', (e) => {
                    e.preventDefault();
                    const targetId = link.getAttribute('href');
                    const targetElement = document.querySelector(targetId);
                    if (targetElement) {
                        targetElement.scrollIntoView({ behavior: 'smooth', block: 'start' });
                    }
                });
            });

            // Removed Chart.js initialization for engagementChart

            const partnerNodes = document.querySelectorAll('.partner-node');
            const partnerNameEl = document.getElementById('partner-name');
            const partnerContentEl = document.getElementById('partner-content');

            const partnerData = {
                'addiction-france': {
                    name: 'Addiction France (Planned)',
                    content: `<p class="text-[var(--color-neutral)]"><strong>Nature of Planned Partnership:</strong> Our goal is to work directly with Addiction France to maximize the reach of our application by integrating "No More" into their existing support frameworks and communication channels.</p><p class="mt-2 text-[var(--color-neutral)]"><strong>Expected Impact:</strong> To simplify the reach of professionals to individuals seeking support by including our app as a key tool in their work, thereby expanding access to care and resources.</p>`
                },
                'ameli': {
                    name: 'Ameli (Planned)',
                    content: `<p class="text-[var(--color-neutral)]"><strong>Nature of Planned Partnership:</strong> Exploring mutual promotion and community outreach. We plan to collaborate on social media campaigns targeting specific demographics in need of support.</p><p class="mt-2 text-[var(--color-neutral)]"><strong>Expected Impact:</strong> To extend our reach to niche communities and validate our approach through association with a trusted organization.</p>`
                },
                'doctolib': {
                    name: 'Doctolib (Planned)',
                    content: `<p class="text-[var(--color-neutral)]"><strong>Nature of Planned Partnership:</strong> Initial technical integration discussions are ongoing to create a referral pathway for users seeking professional medical help.</p><p class="mt-2 text-[var(--color-neutral)]"><strong>Expected Impact:</strong> To strengthen our service offering by bridging the gap between digital support and professional healthcare, creating a holistic recovery tool.</p>`
                },
                'influencers': {
                    name: 'Health & Wellness Influencers (Planned)',
                    content: `<p class="text-[var(--color-neutral)]"><strong>Nature of Planned Partnership:</strong> We are identifying and reaching out to authentic influencers in the mental health and recovery space to share their stories and promote "No More".</p><p class="mt-2 text-[var(--color-neutral)]"><strong>Expected Impact:</strong> To drive targeted user acquisition and build social proof through trusted voices in the community.</p>`
                }
            };
            
            partnerNodes.forEach(node => {
                node.addEventListener('click', () => {
                    const partnerId = node.dataset.partner;
                    const data = partnerData[partnerId];

                    partnerNameEl.textContent = data.name;
                    partnerContentEl.innerHTML = data.content;

                    partnerNodes.forEach(n => n.classList.remove('ring-2', 'ring-[var(--color-highlight)]'));
                    node.classList.add('ring-2', 'ring-[var(--color-highlight)]');
                });
            });
        });
    </script>
</body>
</html>
