    <style>
        :root {
            --color-background-light: #fff5e6;
            --color-primary-text: #664229;
            --color-accent-light: #ffb84d;
            --color-accent-medium: #ff9900;
            --color-accent-dark: #e65c00;
            --color-border: #cd4d00;
            --color-highlight: #ff8533;
            --color-neutral: #994d00;
        }

        body {
            font-family: 'Inter', sans-serif;
            background-color: var(--color-background-light);
            color: var(--color-primary-text);
        }
        .nav-link {
            transition: all 0.3s ease;
            border-bottom: 2px solid transparent;
        }
        .nav-link.active {
            border-bottom-color: var(--color-accent-dark);
            color: var(--color-accent-dark);
        }
        .objective-card {
            transition: transform 0.3s ease, box-shadow 0.3s ease;
        }
        .objective-card:hover {
            transform: translateY(-5px);
            box-shadow: 0 10px 15px -3px rgb(0 0 0 / 0.1), 0 4px 6px -4px rgb(0 0 0 / 0.1);
        }
        .content-section {
            display: none;
        }
        .content-section.active {
            display: block;
        }
        .partner-node {
            transition: all 0.3s ease;
            cursor: pointer;
        }
        .partner-node:hover {
            transform: scale(1.05);
            background-color: var(--color-background-light); /* Lighter hover for partner nodes */
        }
        .partner-line {
            position: absolute;
            background-color: var(--color-neutral);
            z-index: -1;
        }
    </style>
</head>
